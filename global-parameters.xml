<?xml version="1.0" encoding="UTF-8"?>
<parameters>
  <!-- HTML site navigation -->
  <parameter>
    <name>topNav</name>
    <value on="yes">default</value>
    <value>ddbdp</value>
  </parameter>
  
  <parameter>
    <name>leiden-style</name>
    <value>ddbdp</value>
    <value>dohnicht</value>
    <value>edh-itx</value>
    <value>edh-names</value>
    <value>edh-web</value>
    <value>ila</value>
    <value>iospe</value>
    <value>london</value>
    <value on="yes">panciera</value>
    <value>petrae</value>
    <value>rib</value>
    <value>seg</value>
    <value>sammelbuch</value>
  </parameter>
  
  <parameter>
    <name>edn-structure</name>
    <value on="yes">default</value>
    <value>ddbdp</value>
    <value>hgv</value>
    <value>inslib</value>
    <value>iospe</value>
    <value>edh</value>
    <value>edh-db</value>
    <value>rib</value>
    <value>sammelbuch</value>
  </parameter>
  
  <parameter>
    <name>verse-lines</name>
    <value on="yes">off</value><!-- print epigraphic lines at all times, whether text in ab or lg/l -->
    <value>on</value><!-- print verse lines when text in lg/l; interpret gaps as metre where so tagged -->
  </parameter>
  
  <parameter>
    <name>edition-type</name>
    <value on="yes">interpretive</value>
    <value>diplomatic</value>
  </parameter>
  
  <parameter>
    <name>internal-app-style</name>
    <value on="yes">none</value>
    <value>ddbdp</value><!-- NB: change this parameter locally if you're running PN or SoSOL process! -->
    <value>iospe</value>
  </parameter>
  
  <parameter>
    <name>external-app-style</name>
    <value on="yes">default</value>
    <value>iospe</value>
  </parameter>
  
  <parameter><!-- DEPRECATED -->
    <name>apparatus-style</name>
    <value on="yes">default</value>
  </parameter>
  
  <!-- Line-numbering increment -->
  <parameter>
    <name>line-inc</name>
    <value>5</value>
  </parameter>
  
  <parameter>
    <name>css-loc</name>
    <value>../xsl/global.css</value>
  </parameter>
  
<parameter>
    <name>js-dir</name>
    <value>../js/</value>
  </parameter>
  
</parameters>
